<div class="container">
  <div class="row">
    <div id="minor" class="span3">
    <%= render_cell :sidebar, :all %> 
    </div>
    <div id="main" class="span9">
      <ul id="friend_links">
        <% @links.each do |link| %>
          <li class="well clearfix">
            <div class="link">
              <%= link_to image_fu(link.logo, nil), link.url%>
              <%= link_to link.name, link.url %>
            </div>
          </li>
        <% end %>
      </ul>
    </div>
    </div>
</div>
