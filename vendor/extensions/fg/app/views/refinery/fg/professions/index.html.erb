<% content_for :body_content_left do %>
  <ul id="professions">
    <% @professions.each do |profession| %>
      <li>
        <%= link_to profession.name, refinery.fg_profession_path(profession) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>

<% content_for :js do %>
  <%= javascript_include_tag 'jquery-ui.min.js'%>
  <script>
    jQuery().ready(function(){
      $('#courseList').accordion({
        autoHeight: false,
        navigation: true,
        alwaysOpen: false,
        active: false,
        header: '.title3'
      });
    });
  </script>
<% end %>
