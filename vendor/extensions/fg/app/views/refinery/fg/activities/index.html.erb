<div class="container">

  <div class="BoardLayout" id="wrapper">
    <div id="ColumnContainer" style="height: 885px;">
      <div style="display: none;" class="feed">
      </div>
      <% @activities.each do |activity| %>
      <div data-height="400" data-width="500" data-id="<%= activity.id %>" class="pin">
        <div class="PinHolder">
          <%= link_to(style_image_fu(activity.main_image, "205x"), refinery.fg_activity_path(activity), :class => "PinImage ImgLink")%>
        </div>
        <p class="description"><%= activity.desc %></p>
      </div>      
      <% end %>
    </div><!-- #ColumnContainer -->


  </div><!-- #wrapper -->      
</div>
