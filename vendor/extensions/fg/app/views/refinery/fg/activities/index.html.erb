<div class="container">

  <div class="BoardLayout" id="wrapper">
    <div id="ColumnContainer" style="height: 885px;">
      <div style="display: none;" class="feed">
      </div>
      <% @activities.each do |activity| %>
      <div data-height="400" data-width="500" data-id="1" class="pin">
        <div class="PinHolder">
          <%= link_to(image_fu(activity.main_image, "230x250"), "#", :class => "PinImage ImgLink")%>
        </div>
        <p class="description"><%= activity.desc %></p>
      </div>      
      <% end %>
    </div><!-- #ColumnContainer -->


  </div><!-- #wrapper -->      
</div>

<% content_for :javascripts do %>
  <%= javascript_include_tag 'j' %>
  <%= javascript_include_tag 'bundle' %>
<% end %>
