      </a>
      <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=250838451&site=qq&menu=yes">
        <%= image_tag "http://wpa.qq.com/pa?p=2:250838451:45" %>
        <span class="qqTitle">许老师</span>
      </a>
      <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=1318024583&site=qq&menu=yes">
        <%= image_tag "http://wpa.qq.com/pa?p=2:1318024583:45" %>
        <span class="qqTitle">葛老师</span>
      </a>
    </div>
    <script type="text/javascript" charset="utf-8" src="http://static.bshare.cn/b/buttonLite.js#style=-1&amp;uuid=&amp;pophcol=2&amp;lang=zh"></script>
    <script type="text/javascript" charset="utf-8" src="http://static.bshare.cn/b/bshareC0.js"></script>
  </body>
</html>
